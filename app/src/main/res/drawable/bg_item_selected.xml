<?xml version="1.0" encoding="UTF-8"?>
<inset xmlns:android="http://schemas.android.com/apk/res/android"
       android:insetBottom="0.0px"
       android:insetLeft="0.0px"
       android:insetRight="0.0px"
       android:insetTop="0.0px">

    <selector>


        <item android:state_pressed="true">
            <shape>
                <gradient
                    android:angle="270.0"
                    android:endColor="@color/whiteSmoke"
                    android:centerColor="@color/whiteSmoke"
                    android:startColor="@color/whiteSmoke"

                    />

                <corners
                    android:radius="0.0dip"
                    />
            </shape>
        </item>
        <item>
            <shape>
                <stroke
                    android:width="0.0px"
                   />

                <gradient
                    android:angle="270.0"
                    android:endColor="#ffffff"
                    android:centerColor="#ffffff"
                    android:startColor="#ffffff"
                    />

                <corners

                    android:radius="0.0dip"
                    />
            </shape>
        </item>
    </selector>

</inset>